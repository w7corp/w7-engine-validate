{
    "name": "w7/engine-validate",
    "description": "增强表单验证",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "虞灪",
            "email": "995645888@qq.com"
        }
    ],
    "scripts": {
        "test" : "phpunit --configuration phpunit.xml"
    },
    "keywords": [
        "w7",
        "validate",
        "validator",
        "form validate"
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "illuminate/validation": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "laravel-lang/lang": "^8.0",
        "phpunit/phpunit": "^8.5",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "W7\\Validate\\" : "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "W7\\Tests\\" : "tests/"
        }
    },
    "extra": {
        "rangine": {
            "providers": [
                "W7\\Validate\\Providers\\Rangine\\ValidateProvider"
            ]
        },
        "laravel": {
            "providers": [
                "W7\\Validate\\Providers\\Laravel\\ValidateProvider"
            ]
        }
    }
}
